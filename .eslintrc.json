{
    "extends": "eslint:recommended",
    "plugins": [
        "import"
    ],
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {
      "ENV": true
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "never"
        ],
        "no-console": 0
    }
}
